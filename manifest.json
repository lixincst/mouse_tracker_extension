{
    "manifest_version": 2,
    
    "name": "Plugin",
    "description": "This is my first chrome extension",
    "version": "1.0",
    
    "permissions":[
        "tabs",
        "<all_urls>"
    ],
    
    "background": {
    "scripts": ["jquery-1.12.1.min.js","background.js"],
    "persistent": true
  },
    
    "browser_action":{
        "default_title": "Click here",
        "default_popup": "1.html"
    },
    
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-1.12.1.min.js", "content.js"],
      "run_at": "document_end"
    }
  ]
    
}